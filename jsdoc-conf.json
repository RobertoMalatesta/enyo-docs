{
	"tags": {
		"allowUnknownTags": true
	},
	"source": {
		"include": [
			"../enyo/source"
		],
		"includePattern": ".+\\.js(doc)?$",
		"excludePattern": "package\\.js"
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./output",
		"recurse": true,
		"private": false,
		"lenient": false,
		"template": "."
	},
	"plugins": [
		"plugins/markdown",
		"ext/glossary",
		"ext/utility",
		"ext/ui"
	],
	"markdown": {
		"parser": "gfm",
		"hardwrap": false
	}
}